<table>
    <thead>
        <tr>
            <td>Banco</td>
            <td>Agencia</td>
            <td>Saldo</td>
            <td>CPF</td>
        </tr>
    </thead>
    <tbody>
        <%@accounts.each do |conta| %>
        <tr>
            <td><%=conta.banco %></td>
            <td><%=conta.agencia %></td>
            <td><%=conta.saldo %></td>
            <td><%=conta.cpf %></td>
        </tr>
        <%end%>
    </tbody>
</table>